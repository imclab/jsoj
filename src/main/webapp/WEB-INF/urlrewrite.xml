<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">
	<rule>
		<from>/robots.txt</from>
		<to>/robots.txt</to>
	</rule>
	<rule>
		<from>/favicon.ico</from>
		<to>/favicon.ico</to>
	</rule>
	<rule>
		<from>/googlehostedservice.html</from>
		<to>/googlehostedservice.html</to>
	</rule>
	<rule>
		<from>/_ah/**</from>
		<to>/_ah/$1</to>
	</rule>
	<rule>
		<from>/robots.txt</from>
		<to>/robots.txt</to>
	</rule>
	<rule>
		<from>/remote_api</from>
		<to>/remote_api</to>
	</rule>
	<rule>
		<from>/dwr/**</from>
		<to>/dwr/$1</to>
	</rule>
	<rule>
		<from>/images/**</from>
		<to>/images/$1</to>
	</rule>
	<rule>
		<from>/javascripts/**</from>
		<to>/javascripts/$1</to>
	</rule>
	<rule>
		<from>/stylesheets/**</from>
		<to>/stylesheets/$1</to>
	</rule>
	<rule>
		<from>/common/**</from>
		<to>/common/$1</to>
	</rule>
	<rule>
		<from>/**</from>
		<to>/app/$1</to>
	</rule>
	<outbound-rule>
		<from>/app/**</from>
		<to>/$1</to>
	</outbound-rule>
</urlrewrite>